<hibernate-mapping>
    <class name="com.mycompany.pojo.Venue" table="venue">
        <id name="venue_id" type="int">
            <generator class="native"/>
        </id>
        <property name="venue_name" type="string" not-null="true" />
        <property name="venue_city" type="string" not-null="true" />
        <property name="venue_state" type="string" not-null="true" />
        <property name="venue_country" type="string" not-null="true" />
    </class>
</hibernate-mapping>
